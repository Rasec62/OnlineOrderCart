@model OnlineOrderCart.Common.Entities.TypeofPayments

<div class="form-group">
    <label asp-for="PaymentName" class="control-label"></label>
    <input asp-for="PaymentName" class="form-control" placeholder="Tipo de Pago" style="float: left;text-align: center;border-radius: 60px;"/>
    <span asp-validation-for="PaymentName" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="CodeKey" class="control-label"></label>
    <input asp-for="CodeKey" class="form-control" placeholder="Codigo de Tipo de Pago" style="float: left;text-align: center;border-radius: 60px;"/>
    <span asp-validation-for="CodeKey" class="text-danger"></span>
</div>