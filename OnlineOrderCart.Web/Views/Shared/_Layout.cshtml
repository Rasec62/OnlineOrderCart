<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Online Order Cart</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap-glyphicons.css">
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/font-awesome/css/fontawesome.css" rel="stylesheet" />
    <link href="~/font-awesome/css/all.css" rel="stylesheet" />
   
</head>
<body>
    <partial name="_HeaderNavBar" />
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
    <partial name="_footerNavBar" />
    <script src="~/lib/jquery/dist/jquery.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/pleaseWait.js"></script>
    @*<script src="~/lib/jquery/jquery.js"></script>
        <script src="~/lib/jquery/jquery.min.js"></script>*@
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

    <script src="~/font-awesome/js/fontawesome.js"></script>
    <script src="~/font-awesome/js/all.js"></script>
    @*<script src="~/js/SimpleCascading.js"></script>*@
    <script src="~/js/JavaScript.js" asp-append-version="true"></script>
    <script src="~/js/SalesJavaScript.js" asp-append-version="true"></script>
    @*<script src="~/js/PartialJavaScript.js" asp-append-version="true"></script>*@
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
    <ejs-scripts></ejs-scripts>
</body>
</html>
